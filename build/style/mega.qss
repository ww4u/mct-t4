QMenu {
      background-color: #2c3d4f; /* sets background of the menu */
      border: 1px solid #1f2a3b;
	  border-radius: 4px;
	  font: 15px;
  }

QMenu::item{
      padding: 5px 25px;
  }  
  
 QMenu::item:selected { /* when user selects item using mouse or keyboard */
      background-color: #00a3d9;
	  border-radius: 4px;
  }

 QMenu::item:disabled { /* when user selects item using mouse or keyboard */
      background-color: gray;
	  border-radius: 4px;
  }
  
  /*#00a3d9 #2c3d4f*/
 QMenu::icon { /* appearance of a 'checked' icon */
      background: #1a5975;
	  border:none;

      position: absolute;

      top: 1px;
      right: 1px;
      bottom: 1px;
      left: 1px;
  }

  QMenu::icon:checked { /* appearance of a 'checked' icon */
      background: #1a5975;
	  border: 1px inset #00a3d9;
	  border-radius:2px;
      position: absolute;
      top: 1px;
      right: 1px;
      bottom: 1px;
      left: 1px;
  }
  
  
QStatusBar::item {
      border: none;
      
  }

  QSlider::groove:horizontal {
      border: 1px solid #1f2a3b;
      height: 8px; /* the groove expands to the size of the slider by default. by giving it a height, it has a fixed size */
      /*background: #1a5975;*/
	  background: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:1 #000000, stop:0 #004080);
      margin: 2px 0;
	  border-radius: 2px;
  }

  QSlider::handle:horizontal {
      
      border: 2px solid #1a5975;
	  background: #1a5975;
      width: 24px;
      margin: -5px 0; /* handle is placed by default on the contents rect of the groove. Expand outside the groove */
      border-radius: 3px;
  }
  
  QSlider::handle:horizontal:hover {
	background: #00a0e0;
  }
  QSlider::handle:horizontal:pressed {
	border-color: #c5e6f6;
  }

  
  QSlider::groove:horizontal:disabled {
      border: 1px solid #999999;
      height: 2px; /* the groove expands to the size of the slider by default. by giving it a height, it has a fixed size */
      background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #B1B1B1, stop:1 #c4c4c4);
      margin: 2px 0;
  }

  QSlider::handle:horizontal:disabled {
      background: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0 #b4b4b4, stop:1 #8f8f8f);
      border: 1px solid #5c5c5c;
      width: 2px;
      margin: -10px 0; /* handle is placed by default on the contents rect of the groove. Expand outside the groove */
      /* border-radius: 3px; */
  }

  
